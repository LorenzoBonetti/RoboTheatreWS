<launch>
	<!--Run Move_Base-->
        <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
		<!--Load general parameters-->
	    	<rosparam file="$(find triskar_navigation)/params/move_base_general_params.yaml" command="load" />                 
		
		<!--Load global planner parameters-->
  		<rosparam file="$(find triskar_navigation)/params/navfn_params.yaml" command="load" />

 		!--Load local planner parameters-->
		<rosparam file="$(find triskar_navigation)/params/base_local_planner_params.yaml" command="load" />

                <!--Load local costmap  parameters-->
		<rosparam file="$(find triskar_navigation)/params/local_costmap_params.yaml" command="load" />

		<!--Load global costmap  parameters-->
		<rosparam file="$(find triskar_navigation)/params/global_costmap_params.yaml" command="load" /> 
		
		<remap from="/cmd_vel" to="/move_base/published_cmd_vel"/>	
        </node>	
</launch>
