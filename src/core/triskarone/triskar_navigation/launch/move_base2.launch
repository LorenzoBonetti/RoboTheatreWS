<launch>
	<master auto="start"/>
	
	<!-- Run the map server -->

	<node name="map_server" pkg="map_server" type="map_server" args="$(find triskar)/config/airlab_theatre_new.yaml" />

	
	<!-- Run AMCL -->
	<include file="$(find triskar_navigation)/launch/amcl_standard.launch" />

	<!--Move Base-->
	<node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
	        <rosparam file="$(find triskar_navigation)/config/move_base_general_params.yaml" command="load" />
		<rosparam file="$(find triskar_navigation)/config/costmap_common_params.yaml" command="load" ns="global_costmap" />
		<rosparam file="$(find triskar_navigation)/config/costmap_common_params.yaml" command="load" ns="local_costmap" />
		<rosparam file="$(find triskar_navigation)/config/local_costmap_params.yaml" command="load" />
		<rosparam file="$(find triskar_navigation)/config/global_costmap_params.yaml" command="load" />
		<rosparam file="$(find triskar_navigation)/config/base_local_planner_params.yaml" command="load" />

		<!--<remap from="/cmd_vel" to="/game_navigation/unsafe/cmd_vel"/>  TBD REMOVE THIS-->
	</node>
	
</launch>
